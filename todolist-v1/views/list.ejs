<%- include('header'); %>

<div class="box" id="heading">
  <h1><%= listTitle %></h1>
</div>

<div class="box">
  <% for (item of newListItems) { %>
  <div class="item">
    <input type="checkbox" />
    <p><%= item %></p>
  </div>
  <% } %>

  <form action="/" method="POST">
    <input
      type="text"
      name="newItem"
      placeholder="New Item"
      autocomplete="off"
    />
    <button type="submit" name="submit">+</button>
  </form>
</div>

<script src="" async defer></script>

<%- include('footer'); %>
